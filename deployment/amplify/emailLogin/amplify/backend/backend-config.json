{
	"auth": {
		"userPoolGroups": {
			"service": "Cognito-UserPool-Groups",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "auth",
					"resourceName": "echolocatorDevEmailAuth",
					"attributes": [
						"UserPoolId",
						"AppClientIDWeb",
						"AppClientID",
						"IdentityPoolId"
					]
				}
			]
		},
		"echolocatorDevEmailAuth": {
			"service": "Cognito",
			"providerPlugin": "awscloudformation",
			"dependsOn": []
		}
	},
	"storage": {
		"echolocatorDevEmailStorage": {
			"service": "S3",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "auth",
					"resourceName": "echolocatorDevEmailAuth",
					"attributes": [
						"UserPoolId"
					]
				},
				{
					"category": "auth",
					"resourceName": "userPoolGroups",
					"attributes": [
						"counselorsGroupRole"
					]
				}
			]
		}
	},
	"function": {
		"echolocatorDevEmailClientsLambda": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "auth",
					"resourceName": "echolocatorDevEmailAuth",
					"attributes": [
						"UserPoolId"
					]
				}
			]
		}
	},
	"api": {
		"echolocatorDevEmailApi": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "echolocatorDevEmailClientsLambda",
					"attributes": [
						"Name",
						"Arn"
					]
				},
				{
					"category": "auth",
					"resourceName": "echolocatorDevEmailAuth",
					"attributes": [
						"UserPoolId"
					]
				},
				{
					"category": "auth",
					"resourceName": "userPoolGroups",
					"attributes": [
						"counselorsGroupRole"
					]
				}
			]
		}
	}
}