{
  "name": "@azavea/echo-locator",
  "description": "Accessibility analysis for housing",
  "main": "src/index.js",
  "scripts": {
    "build": "mastarm build",
    "fixlint": "mastarm lint --ignore-path src/.eslintignore --fix src",
    "deploy": "mastarm deploy",
    "lint": "mastarm lint src --ignore-path src/.eslintignore && mastarm lint-messages",
    "predeploy": "yarn",
    "prestart": "yarn",
    "pretest": "yarn",
    "semantic-release": "semantic-release",
    "start": "mastarm build --serve --watch",
    "test": "mastarm lint src --ignore-path src/.eslintignore && mastarm build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azavea/echo-locator.git"
  },
  "keywords": [
    "taui",
    "transit"
  ],
  "author": "Azavea",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/azavea/echo-locator/issues"
  },
  "homepage": "https://github.com/azavea/echo-locator",
  "dependencies": {
    "@conveyal/gridualizer": "~2.1.0",
    "@conveyal/lonlat": "~1.4.0",
    "@conveyal/woonerf": "~4.1.0",
    "@mapbox/polyline": "~1.0.0",
    "aws-amplify": "~1.1.19",
    "aws-amplify-react": "~2.1.7",
    "babel-plugin-css-modules-transform": "~1.6.2",
    "d3-scale": "~2.2.2",
    "date-fns": "~1.29.0",
    "debug": "~4.1.0",
    "isomorphic-fetch": "~2.2.1",
    "jsolines": "~2.1.0",
    "leaflet": "~1.3.4",
    "lodash": "~4.17.11",
    "mastarm": "https://github.com/azavea/mastarm.git#dev",
    "react": "~16.8.1",
    "react-dom": "~16.8.1",
    "react-leaflet": "~1.9.1",
    "react-leaflet-vectorgrid": "~2.1.3",
    "react-redux": "~5.0.7",
    "react-router": "~4.3.1",
    "react-router-dom": "~4.3.1",
    "react-select": "~1.2.1",
    "react-select-fast-filter-options": "~0.2.3",
    "react-virtualized-select": "~3.1.3",
    "redux": "~4.0.1",
    "redux-actions": "~2.6.4",
    "redux-mock-store": "1.5.3",
    "reselect": "~4.0.0"
